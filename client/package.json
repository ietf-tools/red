{
  "name": "rfced-www",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "cleanup": "nuxt cleanup",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview --port 4567",
    "test:story": "LOST_PIXEL_DISABLE_TELEMETRY=1 npx --no lost-pixel docker",
    "test:story:view": "npx --no -c 'vite dev ./visual-regression-viewer'",
    "test:story:approve": "LOST_PIXEL_DISABLE_TELEMETRY=1 npx --no lost-pixel docker update",
    "types": "npx nuxi typecheck",
    "generate-content-metadata": "node --experimental-strip-types ./scripts/generate-content-metadata.ts"
  },
  "//": "If updating lost-pixel sync the version to the vis-reg-test.yml file or rendering / filenames generated etc may be different",
  "dependencies": {
    "@ark-ui/vue": "^3.13.0",
    "@nuxt/content": "^2.13.4",
    "@nuxt/fonts": "^0.7.2",
    "@nuxt/icon": "^1.5.0",
    "@nuxtjs/color-mode": "^3.4.4",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@pinia/nuxt": "^0.5.4",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/forms": "^0.5.8",
    "glob": "^11.0.0",
    "lodash-es": "^4.17.21",
    "lost-pixel": "3.18.2",
    "luxon": "^3.5.0",
    "nuxt": "^3.13.1",
    "nuxt-headlessui": "^1.2.0",
    "pinia": "^2.2.2",
    "playwright": "^1.47.1",
    "simple-git": "^3.27.0",
    "vue": "^3.5.4"
  },
  "devDependencies": {
    "@histoire/plugin-nuxt": "^0.17.17",
    "@histoire/plugin-vue": "^0.17.17",
    "@nuxt/eslint": "^0.5.3",
    "@types/lodash-es": "^4.17.12",
    "@types/luxon": "^3.4.2",
    "vue-tsc": "^2.0.29"
  },
  "peerDependencies": {
    "typescript": "^5.5.4"
  }
}
